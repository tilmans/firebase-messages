{
  "name": "firebase-messages",
  "version": "0.0.0-alpha.0",
  "scripts": {
    "start": "firebase serve",
    "build": "webpack",
    "buildWatch": "webpack --watch",
    "deploy": "firebase deploy"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "firebase-bolt": "^0.8.2",
    "firebase-tools": "^3.6.0",
    "webpack": "^1.13.1",
    "html-webpack-plugin": "^2.17.0",
    "elm": "^0.18.0",
    "elm-hot-loader": "^0.3.2",
    "elm-webpack-loader": "^3.0.3",
    "autoprefixer": "^6.3.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^3.0.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "arrow-parens": ["error", "as-needed"],
      "comma-dangle": ["error", "always-multiline"],
      "import/no-dynamic-require": "off",
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
      "max-len": ["error", 240],
      "no-param-reassign": ["error", { "props": false }]
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:RomansBermans/firebase-messages.git"
  },
  "bugs": {
    "url": "https://github.com/RomansBermans/firebase-messages/issues"
  },
  "homepage": "https://github.com/RomansBermans/firebase-messages",
  "author": "Romans Bermans <romans.bermans@gmail.com>",
  "license": "ISC"
}
